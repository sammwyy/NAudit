{
  "id": "example",
  "rules": [
    {
      "port": 135,
      "service": "*msrpc*"
    }
  ],
  "actions": [
    {
      "type": "shell",
      "args": "echo 'hello world'",
      "on_success": "This is a test"
    },
    {
      "type": "shell",
      "args": "echo 'its work!'",
      "rules": [
        {
          "prev_stdout": "this gonna fail!!!"
        }
      ],
      "on_success": "Woah, second test passed!",
      "on_rules_miss": {
        "type": "break",
        "reason": "Uh oh, second test failed"
      }
    }
  ]
}
